{
    "extends": "standard",
    "parser": "babel-eslint",

    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },

    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "comma-dangle": 0,
        "space-before-function-paren": ["error", "never"]
    }
}
